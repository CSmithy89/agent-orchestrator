{
  "name": "@agent-orchestrator/dashboard",
  "version": "1.0.0",
  "description": "Dashboard UI for the Agent Orchestrator",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",

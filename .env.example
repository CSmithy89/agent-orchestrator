# LLM Provider Authentication (multi-provider support)
# Anthropic Claude (preferred method: OAuth, fallback: API key)
CLAUDE_CODE_OAUTH_TOKEN=your_claude_code_oauth_token_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI GPT/Codex models
OPENAI_API_KEY=your_openai_api_key_here

# Zhipu GLM (optional - native access)
ZHIPU_API_KEY=your_zhipu_api_key_here

# GLM via z.ai Anthropic-compatible wrapper (optional)
ZAI_API_KEY=your_zai_api_key_here
ZAI_BASE_URL=https://api.z.ai/api/anthropic

# Google Gemini models (optional - future support)
GOOGLE_API_KEY=your_google_api_key_here

# GitHub Integration
GITHUB_TOKEN=your_github_token_here

# Authentication
JWT_SECRET=your_jwt_secret_here_min_32_chars

# Application Configuration
NODE_ENV=development
PORT=3000
API_BASE_URL=http://localhost:3000

# Database (future use)
# DATABASE_URL=postgresql://user:password@localhost:5432/agent_orchestrator

# Logging
LOG_LEVEL=info

# Feature Flags
ENABLE_ESCALATIONS=true
ENABLE_AUTONOMOUS_MODE=true
